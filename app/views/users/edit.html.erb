<% provide(:title, "Update") %>
<h1>Sign Up</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		
		<%= form_for(@user) do |f| %>
      	  <%= render 'shared/error_messages', object: f.object %>
		  <%= render(:partial => 'form', :locals => {:f => f})%>
			<%= f.submit("Update", class: "btn btn-primary")%>
		<% end %>

	</div>
</div>